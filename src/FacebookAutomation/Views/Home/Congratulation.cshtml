<script>
(function($) {
	"use strict";

	//
	// Preload images
	//

	$('document').ready(function() {

		var $container = $('body'),
			tweetsTimer;

		$container.imagesLoaded( function() {

			$('#faq-sorting').rivaSorting({
				selector	: 'item',
				gutter		: 0,
				inRow		: 1
			});

			$('#spinner-wrapper').hide();

		});

		clearTimeout(tweetsTimer);

	});

})(jQuery);
</script>

}

