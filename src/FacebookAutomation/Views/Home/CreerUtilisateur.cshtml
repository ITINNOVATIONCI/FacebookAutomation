<script>
(function($) {
	"use strict";

	//
	// Preload images
	//

	$('document').ready(function() {

		var $container = $('body'),
			tweetsTimer;

		$container.imagesLoaded( function() {

			$('#spinner-wrapper').hide();

		});

		clearTimeout(tweetsTimer);

		$('#sendMessageBtn').click(function(e) {
			e.preventDefault();
			processSendEmail();
		});

	});

})(jQuery);
</script>

